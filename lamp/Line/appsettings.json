{
  "ReflectionConfig": [
    {
      "Files": [
        "RaGae.ArgumentLib.StringMarshalerLib.dll",
        "RaGae.ArgumentLib.IntegerMarshalerLib.dll"
      ]
    }
  ],
  "ArgumentConfig": {
    "Schema": [
      {
        "Argument": [
          "c",
          "company"
        ],
        "Marshaler": "*",
        "Required": true
      },
      {
        "Argument": [
          "r",
          "repository"
        ],
        "Marshaler": "*",
        "Required": true
      },
      {
        "Argument": [
          "v",
          "version"
        ],
        "Marshaler": "*",
        "Required": true
      },
      {
        "Argument": [
          "d",
          "directory"
        ],
        "Marshaler": "*",
        "Required": true
      },
      {
        "Argument": [
          "p",
          "process"
        ],
        "Marshaler": "#",
        "Required": true
      }
    ],
    "Delimiter": "-:/"
  }
}